{
  "name": "PC-Jackal",
  "version": "1.0",
  "description": "HackCU 2020 Project - Converts the highlighted number into different bases.",
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs", "<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.jpg",
      "48": "images/icon48.png",
      "128": "images/icon128.jpg"
    }
  },
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["selection_injection.js"],
     "run_at": "document_start",
     "all_frames": true
   }
 ],
 "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.jpg",
      "48": "images/icon48.png",
      "128": "images/icon128.jpg"
    }
}